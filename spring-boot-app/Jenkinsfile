pipeline{
    agent{
        docker{
            image 'ujnvdprasad/slave-image:v1'
            args '--user root -v /var/lib/docker.sock:/var/lib/docker.sock'
        }
    }
    stages {
    stage('Checkout') {
      steps {
        sh 'echo passed',
        git branch: 'master', url: 'https://github.com/ujnvdprasad/Jenkins-Docker-SonarQube.git'
      }
}
